//DSL file which is equivalent to the built-in test configuration 1 in config.h. Also used to test that a
//DSL file with the same configuration as a built-in ActionSet definition has the same output.

Output: int 

Attributes:
	(i) "STRING" ,
	(i) "FINGER" ,
	(i) "HAND_POS" ;

Strings:
	1 : G_3 -> Gs_5 , 
	2 : D_4 -> Ds_6 , 
	3 : A_4 -> As_6 , 
	4 : E_5 -> F_7 ;

Actions:
"REST_F" = this "STRING" == 0 && this "FINGER" == 0 && this "HAND_POS" == 0 : 0 (true) ,
"REST_T" = to "STRING" == 0 && to "FINGER" == 0 && to "HAND_POS" == 0 : 0 (true) , 
"HP" = "HAND_POS" - "HAND_POS" != 0 : "HAND_POS" - "HAND_POS" (true) ,
"FI" = "FINGER" - "FINGER" != 0 : "FINGER" - "FINGER" (true) , 
"ST_NR" = "STRING" - "STRING" >= 2 : "FINGER" - "FINGER" + 100 (true) , 
"ST" = "STRING" - "STRING" > 0 : "STRING" - "STRING" (true) ;

Dependencies:
"HP" "REST_F" false , 
"FI" "REST_F" false ,
"ST_NR" "REST_F" false ,
"ST" "REST_F" false ,
"HP" "REST_T" false , 
"FI" "REST_T" false ,
"ST_NR" "REST_T" false ,
"ST" "REST_T" false ,
"ST" "ST_NR" false ;
